{
  "name": "node-red-contrib-rfxcom",
  "version": "2.5.0",
  "description": "A set of node-red nodes to use the Rfxcom RFXtrx433 remote control transceiver",
  "author": {
    "name": "Maxwell Hadley",
    "email": "max@zepler.org"
  },
  "engines" : { "node" : ">=4.8.0" },
  "repository": {
    "type": "git",
    "url": "https://github.com/maxwellhadley/node-red-contrib-rfxcom.git"
  },
  "scripts": {
    "find-rfxcom": "./node_modules/.bin/find-rfxcom",
    "set-protocols": "./node_modules/.bin/set-protocols"
  },
  "bin": {
    "find-rfxcom": "./node_modules/.bin/find-rfxcom",
    "set-protocols": "./node_modules/.bin/set-protocols"
  },
  "license": "BSD-2-Clause",
  "keywords": [
    "node-red",
    "rfxcom",
    "RFXtrx433"
  ],
  "node-red": {
    "nodes": {
      "rfxcom": "rfxcom.js"
    }
  },
  "dependencies": {
    "rfxcom": ">=1.3.0 <2.0.0"
  }
}
